{
  "name": "Template",
  "version": "0.0.0",
  "description": "Template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rose-m/project-react-php-template.git"
  },
  "keywords": [
    "vite",
    "react",
    "php",
    "starter",
    "template"
  ],
  "author": "Michael Rose <michael_rose@gmx.de>",
  "license": "MIT",
  "homepage": "https://github.com/rose-m/project-react-php-template",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"yarn dev:frontend\" \"yarn dev:backend\" --names \"Frontend,Backend\" --prefix-colors \"bgBlue.bold,bgGreen.bold\"",
    "dev:frontend": "vite",
    "dev:backend": "php -S localhost:8080",
    "dev:db:schema": "mysql -h 127.0.0.1 -u root -p'rootpassword' template < db/schema.sql",
    "dev:db:reset": "echo 'DROP DATABASE template; CREATE DATABASE template;' | mysql -h 127.0.0.1 -u root -p'rootpassword' template && yarn dev:db:schema",
    "build": "tsc && yarn run clean && vite build && yarn run build:backend",
    "build:backend": "rm -rf ./dist/scripting && cp -R ./scripting ./dist/scripting",
    "test": "jest --passWithNoTests",
    "serve": "vite preview",
    "prepare": "husky install",
    "commit": "commit",
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --cache ./src"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.40.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.5",
    "vite-plugin-svgr": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.19",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vite-tsconfig-paths": "^4.0.1",
    "watch": "^1.0.2"
  }
}
